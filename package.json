{
  "name": "paul-shryock",
  "version": "0.0.0",
  "private": true,
  "description": "Source code for my personal website.",
  "homepage": "https://github.com/paulshryock/paul-shryock#readme",
  "bugs": {
    "url": "https://github.com/paulshryock/paul-shryock/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/paulshryock/paul-shryock.git"
  },
  "license": "Hippocratic 2.1",
  "author": "Paul Shryock <paul@pshry.com> (https://github.com/paulshryock)",
  "type": "module",
  "scripts": {
    "prebuild": "run-s compile lint",
    "build": "node ./scripts/build.js",
    "compile": "tsc",
    "lint": "eslint \"./**/*.{js,json,tx}\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "sort-package-json"
    ],
    "./**/*.{js,ts}": [
      "npm run lint"
    ]
  },
  "devDependencies": {
    "@tsconfig/node16": "^1.0.2",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.32.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-json-format": "^2.0.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.2.2",
    "npm-run-all2": "^5.0.2",
    "sort-package-json": "^1.52.0",
    "typescript": "^4.4.3"
  }
}
